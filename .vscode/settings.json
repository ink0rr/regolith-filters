{
  "deno.enablePaths": [
    "./esbuild_deno",
    "./janitor",
    "./lazuli",
    "./spawn_egg",
    "./sync_manifest"
  ],
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "files.eol": "\n",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
